{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "rootDir": "./src",
    "outDir": "./dist",
    "lib": ["es6", "dom"],
    "esModuleInterop": false,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node"
  }
}
